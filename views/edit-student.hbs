<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item"><a href="/studentdatamanage" method="post">Home</a></li>
        <li class="breadcrumb-item active" aria-current="page">Edit Student Data</li>
    </ol>
</nav>




{{#if alert}}
<div class="alert alert-success alert-dismissible fade show" role="alert">
    {{alert}}

    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
{{/if}}


{{#each rows}}
<form class="row g-3 needs-validation" method="post" action="/editstudent/{{this.rollno}}">


    {{> student-form}}

</form>

{{/each}}