{{#if removedUser}}
<div class="alert alert-success alert-dismissible fade show" role="alert">
    User has been removed
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
</div>
{{/if}}


<div class="col-6 d-flex justify-content-end">
    <a href="/" type="button" class="btn btn-primary align-self-center">Logout</a>

</div>




<div class="row">

    <div class="col-6">
        <h2>Student</h2>
    </div>
    <div class="col-6 d-flex justify-content-end">
        <a href="/adddata" type="button" class="btn btn-primary align-self-center">Add Student</a>

    </div>
</div>




<table class="table table-bordered">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Roll Number</th>
            <th scope="col">Name</th>
            <th scope="col">Date Of Birth</th>
            <th scope="col">Score</th>
            <th scope="col">Action</th>

        </tr>
    </thead>
    <tbody>

        {{#each rows}}


        <tr>
            <th scope="row">{{this.rollno}}</th>
            <td>{{this.name}}</td>
            <td>{{this.dob}}</td>

            <td>{{this.score}}</td>
            <td class="text-end">
                <a href="/editstudent/{{this.rollno}}" type="button" class="btn btn-small"><i class="bi bi-pencil"></i>
                    Edit</a>
                <a href="/{{this.rollno}}" type="button" class="btn btn-small"><i class="bi bi-person-x"></i> Delete</a>


            </td>
        </tr>

        {{/each}}
    </tbody>
</table>